{
  "name": "step-bible-mobile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/react-test-renderer": "^16.0.1",
    "jest-expo": "^37.0.0",
    "metro-minify-terser": "^0.56.3",
    "react-native-scripts": "1.14.0",
    "react-native-typescript-transformer": "^1.2.11",
    "react-test-renderer": "^16.3.1"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node_modules/.bin/jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/netinfo": "5.5.0",
    "@types/react-native-snap-carousel": "^3.7.4",
    "babel-preset-expo": "^8.1.0",
    "elasticlunr": "^0.9.5",
    "expo": "^37.0.0",
    "expo-analytics": "^1.0.11",
    "expo-asset": "~8.1.3",
    "expo-constants": "~9.0.0",
    "expo-file-system": "~8.1.0",
    "expo-font": "~8.1.0",
    "expo-keep-awake": "~8.1.0",
    "expo-sqlite": "~8.1.0",
    "hoist-non-react-statics": "^3.3.0",
    "mobx": "^5.15.0",
    "mobx-react": "5.4.3",
    "mobx-sync": "^2.1.2",
    "react": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.0.tar.gz",
    "react-native-collapsible": "^1.4.0",
    "react-native-console-time-polyfill": "^1.2.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-paper": "^3.4.0",
    "react-native-raw-bottom-sheet": "^2.0.6",
    "react-native-reanimated": "~1.7.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-simple-store": "^1.3.0",
    "react-native-skeleton-placeholder": "^1.0.3",
    "react-native-snap-carousel": "^3.8.4",
    "react-native-sqlite-storage": "^3.3.5",
    "react-navigation": "^4.0.10",
    "react-navigation-collapsible": "^3.0.3",
    "react-navigation-stack": "^1.9.1",
    "sentry-expo": "2.0.1"
  },
  "peerDependencies": {
    "@bible-engine/core": "0.x",
    "@bible-engine/client": "0.x"
  }
