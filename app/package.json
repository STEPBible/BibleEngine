{
  "name": "step-bible-mobile",
  "version": "3.0.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.3",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "^16.0.1",
    "jest-expo": "^44.0.0",
    "metro-minify-terser": "^0.56.3",
    "react-native-scripts": "1.14.0",
    "react-native-typescript-transformer": "^1.2.11",
    "react-test-renderer": "^16.3.1",
    "standard-version": "^9.3.2",
    "standard-version-expo": "^1.0.3",
    "ts-jest": "^22.4.6",
    "typescript": "~4.3.5"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "eas-build-pre-install": "cd .. && yarn install",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node_modules/.bin/jest",
    "build-staging": "EAS_NO_VCS=1 eas build -p=android --non-interactive --profile staging",
    "copy-core": "cd ../core && rimraf ./lib && tsc && cp -R lib/* ../app/node_modules/@bible-engine/core/lib/"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  },
  "dependencies": {
    "@bible-engine/core": "^1.0.1",
    "@expo/metro-config": "^0.3.8",
    "@expo/vector-icons": "^12.0.0",
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/netinfo": "7.1.3",
    "@types/react-native-snap-carousel": "^3.8.4",
    "@unimodules/core": "~7.2.0",
    "babel-preset-expo": "9.0.2",
    "elasticlunr": "^0.9.5",
    "expo": "^44.0.0",
    "expo-analytics": "^1.0.18",
    "expo-application": "~4.0.1",
    "expo-asset": "~8.4.6",
    "expo-constants": "~13.0.1",
    "expo-device": "~4.1.0",
    "expo-file-system": "~13.1.2",
    "expo-font": "~10.0.4",
    "expo-keep-awake": "~10.0.1",
    "expo-sqlite": "~10.1.0",
    "expo-system-ui": "~1.1.0",
    "expo-updates": "~0.11.6",
    "hoist-non-react-statics": "^3.3.0",
    "mobx": "^5.15.0",
    "mobx-react": "5.4.3",
    "mobx-sync": "^2.1.2",
    "react": "17.0.1",
    "react-native": "0.64.3",
    "react-native-console-time-polyfill": "^1.2.1",
    "react-native-dotenv": "^3.2.0",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-paper": "^3.4.0",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-reanimated": "~2.3.1",
    "react-native-render-html": "^6.3.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-simple-store": "^1.3.0",
    "react-native-skeleton-placeholder": "^1.0.3",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-webview": "11.15.0",
    "react-navigation": "^4.4.4",
    "react-navigation-collapsible": "^5.10.2",
    "react-navigation-stack": "^2.10.4",
    "sentry-expo": "^4.0.0",
    "typeorm": "~0.2.34"
  },
  "peerDependencies": {
    "@bible-engine/core": "0.x"
  },
  "expo": {
    "plugins": [
      "sentry-expo"
    ]
  }
}
