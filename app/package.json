{
  "name": "step-bible-mobile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.3",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.57.65",
